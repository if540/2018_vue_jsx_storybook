{
  "presets": [
    ["env", {
      "modules": false,
      "loose": true,
      "targets": {
        "browsers": [
          "> 5%",
          "last 2 versions", 
          "not ie <= 10"
        ]
      }
    }],
    "stage-0",
    "vue"
  ],
  "plugins": [
    "transform-pug-html",
    "jsx-vue-functional"
  ]
}
